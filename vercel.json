{
  "version": 2,
  "buildCommand": "npm install && npm run web:build",
  "outputDirectory": "web-build",
  "devCommand": "npm start",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
