{
  "name": "dating-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "web:build": "expo export -p web",
    "build": "expo export -p web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\" --ignore-path .prettierignore",
    "format:check": "prettier --check \"**/*.{js,jsx,json,md}\" --ignore-path .prettierignore",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "audit": "npm audit",
    "audit:fix": "npm audit fix"
  },
  "dependencies": {
    "expo": "~54.0.30",
    "expo-status-bar": "~3.0.9",
    "react": "18.3.1",
    "react-native": "0.76.5",
    "react-dom": "18.3.1",
    "react-native-web": "^0.21.0",
    "firebase": "^10.13.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "react-native-gesture-handler": "~2.20.0",
    "react-native-reanimated": "~3.16.7",
    "react-native-screens": "~4.4.0",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "react-native-safe-area-context": "4.14.0",
    "expo-image-picker": "~16.0.4",
    "expo-location": "~18.0.4",
    "expo-auth-session": "~6.0.0",
    "expo-web-browser": "~14.0.0",
    "expo-constants": "~17.0.3",
    "expo-notifications": "~0.35.0",
    "expo-device": "~7.0.0",
    "expo-document-picker": "~13.0.0",
    "expo-image-manipulator": "~12.0.0",
    "expo-linear-gradient": "~14.0.1",
    "@expo/vector-icons": "^14.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.2.4",
    "jest": "^29.7.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/react-native": "^12.4.0",
    "@testing-library/jest-native": "^5.4.3",
    "jest-expo": "~51.0.0",
    "@testing-library/jest-dom": "^6.1.5",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "private": true,
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": ["@testing-library/jest-native/extend-expect"],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "coverageReporters": ["text", "lcov", "html"],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(js|jsx|ts|tsx)",
      "<rootDir>/src/**/*.(test|spec).(js|jsx|ts|tsx)"
    ]
  }
}
