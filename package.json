{
  "name": "dating-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "web:build": "npx expo export -p web --output-dir web-build",
    "build": "npx expo export -p web --output-dir web-build",
    "vercel-build": "npx expo export -p web --output-dir web-build",
    "start:backend": "node backend/server.js",
    "seed": "node backend/scripts/seedLocal.js",
    "test": "jest",
    "test:backend": "jest -c backend/jest.config.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:property": "jest --testPathPattern=property",
    "test:all": "jest --testPathPattern='(test|spec)'",
    "test:error-scenarios": "jest --testPathPattern=error-scenarios",
    "test:edge-cases": "jest --testPathPattern=edge-cases",
    "test:e2e:web": "playwright test --project=chromium",
    "test:e2e:web:all": "playwright test",
    "test:e2e:web:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:e2e:web:tablet": "playwright test --project='Tablet iPad' --project='Tablet Android'",
    "test:e2e:web:performance": "playwright test --project=performance",
    "test:e2e:web:error": "playwright test --project=error-scenarios",
    "test:e2e:web:devices": "playwright test --project=device-testing",
    "test:performance": "jest --testPathPattern=performance",
    "test:performance:network": "jest --testPathPattern=network-throttling",
    "test:performance:low-end": "jest --testPathPattern=low-end-device",
    "test:performance:memory": "jest --testPathPattern=memory-leaks",
    "test:responsive": "jest --testPathPattern=responsive",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\" --ignore-path .prettierignore",
    "format:check": "prettier --check \"**/*.{js,jsx,json,md}\" --ignore-path .prettierignore",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "snyk:test": "snyk test --severity-threshold=high --org=4a0071c2-7ef8-4aa0-9bbb-0068b72f03b0",
    "snyk:test:all": "snyk test --org=4a0071c2-7ef8-4aa0-9bbb-0068b72f03b0",
    "snyk:monitor": "snyk monitor --org=4a0071c2-7ef8-4aa0-9bbb-0068b72f03b0",
    "snyk:fix": "snyk test --severity-threshold=high --org=4a0071c2-7ef8-4aa0-9bbb-0068b72f03b0",
    "verify:env": "node scripts/verify-production-env.js",
    "verify:legal": "node scripts/verify-legal-urls.js",
    "test:deletion": "node scripts/test-account-deletion.js",
    "test:monitoring": "node scripts/test-error-monitoring.js",
    "validate:env": "node scripts/validate-env-for-production.js",
    "preproduction:check": "node scripts/production-readiness-check.js && node scripts/validate-env-for-production.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@sentry/node": "^10.32.1",
    "@sentry/react": "^8.0.0",
    "@sentry/react-native": "~7.2.0",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "ai": "^6.0.6",
    "expo": "~54.0.30",
    "expo-auth-session": "~6.0.0",
    "expo-constants": "~17.0.3",
    "expo-device": "~8.0.10",
    "expo-document-picker": "~13.0.0",
    "expo-file-system": "~18.0.4",
    "expo-firebase-analytics": "^8.0.0",
    "expo-image-manipulator": "~12.0.0",
    "expo-image-picker": "~16.0.4",
    "expo-in-app-purchases": "^14.5.0",
    "expo-linear-gradient": "~14.0.1",
    "expo-location": "~18.0.4",
    "expo-notifications": "~0.32.15",
    "expo-secure-store": "^15.0.8",
    "expo-sharing": "~13.0.0",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~14.0.0",
    "firebase": "^11.10.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.5",
    "react-native-gesture-handler": "~2.20.0",
    "react-native-reanimated": "~3.16.7",
    "react-native-safe-area-context": "4.14.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "^0.21.0",
    "socket.io-client": "^4.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-native": "^5.4.0",
    "@testing-library/react-native": "^12.0.0",
    "@types/jest": "^29.5.0",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-no-secrets": "^2.2.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^62.0.0",
    "fast-check": "^4.5.3",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.0",
    "prettier": "^3.2.5",
    "typescript": "~5.9.2"
  },
  "engines": {
    "node": "20.x"
  },
  "private": true
}
