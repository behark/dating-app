{
  "name": "dating-app",
  "version": "1.4.2",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "web:build": "npx expo export -p web --output-dir web-build",
    "build": "npx expo export -p web --output-dir web-build",
    "vercel-build": "npx expo export -p web --output-dir web-build",
    "start:backend": "node backend/server.js",
    "seed": "node backend/scripts/seedLocal.js",
    "test": "jest",
    "test:backend": "jest -c backend/jest.config.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:property": "jest --testPathPattern=property",
    "test:all": "jest --testPathPattern='(test|spec)'",
    "test:error-scenarios": "jest --testPathPattern=error-scenarios",
    "test:edge-cases": "jest --testPathPattern=edge-cases",
    "test:e2e:web": "playwright test --project=chromium",
    "test:e2e:web:all": "playwright test",
    "test:e2e:web:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:e2e:web:tablet": "playwright test --project='Tablet iPad' --project='Tablet Android'",
    "test:e2e:web:performance": "playwright test --project=performance",
    "test:e2e:web:error": "playwright test --project=error-scenarios",
    "test:e2e:web:devices": "playwright test --project=device-testing",
    "test:performance": "jest --testPathPattern=performance",
    "test:performance:network": "jest --testPathPattern=network-throttling",
    "test:performance:low-end": "jest --testPathPattern=low-end-device",
    "test:performance:memory": "jest --testPathPattern=memory-leaks",
    "test:responsive": "jest --testPathPattern=responsive",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\" --ignore-path .prettierignore",
    "format:check": "prettier --check \"**/*.{js,jsx,json,md}\" --ignore-path .prettierignore",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "snyk:test": "snyk test --severity-threshold=high --org=4a0071c2-7ef8-4aa0-9bbb-0068b72f03b0",
    "snyk:test:all": "snyk test --org=4a0071c2-7ef8-4aa0-9bbb-0068b72f03b0",
    "snyk:monitor": "snyk monitor --org=4a0071c2-7ef8-4aa0-9bbb-0068b72f03b0",
    "snyk:fix": "snyk test --severity-threshold=high --org=4a0071c2-7ef8-4aa0-9bbb-0068b72f03b0",
    "verify:env": "node scripts/verify-production-env.js",
    "audit:cloud-env": "node scripts/cloud-env-audit.js",
    "verify:legal": "node scripts/verify-legal-urls.js",
    "test:deletion": "node scripts/test-account-deletion.js",
    "test:monitoring": "node scripts/test-error-monitoring.js",
    "validate:env": "node scripts/validate-env-for-production.js",
    "preproduction:check": "node scripts/production-readiness-check.js && node scripts/validate-env-for-production.js",
    "prepare": "bash scripts/prepare.sh"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@sentry/node": "^10.32.1",
    "@sentry/react": "^8.0.0",
    "@sentry/react-native": "~7.2.0",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "ai": "^6.0.6",
    "axios": "^1.13.5",
    "babel-plugin-module-resolver": "^5.0.2",
    "canvas": "^3.2.1",
    "expo": "~54.0.33",
    "expo-audio": "~1.1.1",
    "expo-auth-session": "~7.0.10",
    "expo-build-properties": "~1.0.10",
    "expo-constants": "~18.0.13",
    "expo-device": "~8.0.10",
    "expo-document-picker": "~14.0.8",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.11",
    "expo-haptics": "~15.0.8",
    "expo-image-manipulator": "~14.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-location": "~19.0.8",
    "expo-notifications": "~0.32.16",
    "expo-secure-store": "~15.0.8",
    "expo-sharing": "~14.0.8",
    "expo-status-bar": "~3.0.9",
    "expo-updates": "~29.0.16",
    "expo-video": "~3.0.16",
    "expo-web-browser": "~15.0.10",
    "firebase": "^11.10.0",
    "prop-types": "^15.8.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.84.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-iap": "^14.7.1",
    "react-native-nitro-modules": "^0.33.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1",
    "socket.io-client": "^4.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-native": "^5.4.0",
    "@testing-library/react": "^16.3.1",
    "@testing-library/react-native": "^12.0.0",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "benchmark": "^2.1.4",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-no-secrets": "^2.2.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^62.0.0",
    "fast-check": "^4.5.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.17",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "typescript": "~5.9.2"
  },
  "engines": {
    "node": "20.x"
  },
  "private": true,
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ],
    "backend/**/*.{js,ts}": [
      "eslint --fix"
    ]
  }
}
